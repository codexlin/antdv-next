{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build:token": {
      "dependsOn": ["@antdv-next/cssinjs#build", "build:esm"],
      "cache": false,
      "outputs": []
    },
    "build:esm": {
      "dependsOn": ["^build:esm", "@antdv-next/icons#build"],
      "outputs": ["dist/**"]
    },
    "@antdv-next/icons#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@antdv-next/cssinjs#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["antdv-next#build:token", "^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["antdv-next#build:token", "^build"],
      "cache": false,
      "persistent": true
    }
  }
}
